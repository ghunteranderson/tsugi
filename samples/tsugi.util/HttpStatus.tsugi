// An enum is a collection of constants. They may have data associated with them
// but that data should be constant
enum HttpStatus:

    FORBIDDEN(403, "Forbidden")
    BAD_REQUEST(400, "Bad Request")
    INTERNAL_SERVER_ERROR(500 "Internal Server Error")
    NOT_FOUND(404, "Not Found")
    FOUND(301, "Found")

    private const Integer status
    private const String message

    init(Integer status, Integer message):
        this.status = status
        this.message = message
